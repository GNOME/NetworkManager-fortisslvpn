msgid "User Key:"
msgstr "Chiave utente:"

#: ../properties/nm-fortisslvpn-dialog.ui.h:18
msgid "User Certificate:"
msgstr "Certificato utente:"

#: ../properties/nm-fortisslvpn-dialog.ui.h:19
msgid "A_dvanced..."
msgstr "A_vanzate…"

#: ../properties/nm-fortisslvpn-dialog.ui.h:20
msgid "Default"
msgstr "Predefinito"

